        <Button x:Name="btn3" Content="3" HorizontalAlignment="Left" Margin="964,540,0,0" VerticalAlignment="Top" Height="79" Background="#FF41B8CB" Width="105" FontSize="30"/>
        <Button x:Name="btn4" Content="4" HorizontalAlignment="Left" Margin="1081,540,0,0" VerticalAlignment="Top" Height="79" Background="#FF41B8CB" Width="105" FontSize="30"/>
